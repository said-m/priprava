{
  "name": "priprava",
  "version": "0.4.6",
  "description": "Template engine for describing JSON with dynamic data.",
  "keywords": [
    "json",
    "template",
    "object"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json",
    "validate": "tsc --noEmit",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jest.config.json",
    "test:coverage": "yarn test --coverage",
    "test-ci": "yarn test:coverage && cat ./coverage/lcov.info | coveralls",
    "prepack": "yarn build",
    "postpack": "rm -rf ./dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "author": "Said Magomedov <saids.inbox@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/said-m/priprava.git"
  },
  "bugs": "https://github.com/said-m/priprava/issues",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.173",
    "@types/node": "^16.9.4",
    "coveralls": "^3.1.1",
    "husky": "^7.0.2",
    "jest": "^27.2.0",
    "ts-jest": "^27.0.5",
    "tslint": "^6.1.3",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@said-m/common": "^1.0.1",
    "iods": "^2.0.0",
    "lodash": "^4.17.21"
  }
}
