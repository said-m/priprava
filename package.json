{
  "name": "priprava",
  "version": "1.0.0",
  "description": "Template engine for describing JSON with dynamic data with variables, conditions and loops.",
  "author": "Said Magomedov <saids.inbox@gmail.com>",
  "license": "MIT",
  "keywords": [
    "json",
    "template",
    "object"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json",
    "validate": "tsc --noEmit",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jest.config.json",
    "test:coverage": "yarn test --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prepare": "husky install",
    "prepack": "yarn build",
    "postpack": "rm -rf ./dist"
  },
  "dependencies": {
    "@said-m/common": "^1.0.1",
    "iods": "^2.0.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.defaults": "^4.2.0",
    "lodash.isboolean": "^3.0.3",
    "lodash.isstring": "^4.0.1",
    "lodash.omit": "^4.5.0",
    "lodash.template": "^4.5.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.defaults": "^4.2.6",
    "@types/lodash.isboolean": "^3.0.6",
    "@types/lodash.isstring": "^4.0.6",
    "@types/lodash.omit": "^4.5.6",
    "@types/lodash.template": "^4.5.0",
    "@types/node": "^16.9.4",
    "coveralls": "^3.1.1",
    "husky": "^7.0.0",
    "jest": "^27.2.0",
    "ts-jest": "^27.0.5",
    "tslint": "^6.1.3",
    "typescript": "^4.4.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/said-m/priprava.git"
  },
  "bugs": "https://github.com/said-m/priprava/issues"
}
