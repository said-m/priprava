{
  "defaultSeverity": "error",
  "extends": [],
  "rules": {
    "prefer-array-literal": false,
    "array-type": {
      "options": "generic"
    },
    "interface-name": [
      true,
      "never-prefix"
    ],
    "interface-over-type-literal": true,
    "space-before-function-paren": [
      true,
      "never"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-rest-spread",
      "check-type",
      "check-typecast",
      "check-type-operator",
      "check-preblock"
    ],
    "max-line-length": [
      true,
      {
        "limit": 100,
        "ignore-pattern": "^import |^export {(.*?)}"
      }
    ],
    "arrow-return-shorthand": true,
    "no-duplicate-imports": true,
    "no-angle-bracket-type-assertion": true,
    "no-conditional-assignment": true,
    "no-non-null-assertion": true,
    "no-unnecessary-qualifier": true,
    "no-string-throw": true,
    "encoding": true,
    "no-floating-promises": true,
    "no-implicit-dependencies": true,
    "align": [
      true,
      "elements",
      "members",
      "parameters",
      "statements"
    ],
    "no-inferrable-types": true,
    "class-name": true,
    "comment-format": [
      true,
      "check-space"
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "eofline": true,
    "import-spacing": true,
    "match-default-export-name": true,
    "newline-before-return": true,
    "no-consecutive-blank-lines": [
      true,
      2
    ],
    "no-duplicate-variable": true,
    "no-eval": true,
    "no-arg": true,
    "no-internal-module": true,
    "no-trailing-whitespace": true,
    "no-unused-expression": true,
    "no-var-keyword": true,
    "one-line": [
      true,
      "check-catch",
      "check-else",
      "check-finally",
      "check-open-brace",
      "check-whitespace"
    ],
    "ordered-imports": true,
    "prefer-const": true,
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "semicolon": [
      true,
      "always"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "curly": true,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "object-literal-key-quotes": [
      true,
      "as-needed"
    ]
  },
  "rulesDirectory": []
}
